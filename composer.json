{
  "name": "alancting/oauth2-microsoft-adfs",
  "version": "1.0.0",
  "description": "Microsoft ADFS OpenID/OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "alancting",
      "homepage": "https://github.com/alancting/oauth2-microsoft-adfs"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "microsoft",
    "php",
    "symfony",
    "openid",
    "adfs"
  ],
  "repositories": [
    {
      "type": "path",
      "url": "/var/www/local_vendor/php-microsoft-jwt"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^4.3|^5.0",
    "league/oauth2-client": "^2.0",
    "knpuniversity/oauth2-client-bundle": "^2.3",
    "symfony/dependency-injection": "^4.4|^5.0",
    "alancting/php-microsoft-jwt": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.3.3"
  },
  "autoload": {
    "psr-4": {
      "Alancting\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alancting\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  },
  "scripts": {
    "coverage": ["rm -rf ./build/coverage", "phpunit --coverage-text --coverage-html ./build/coverage/html --coverage-clover ./build/coverage/clover/clover.xml"]
  }
}
